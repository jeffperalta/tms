	<queryString><![CDATA[SELECT a_users.user_id, CONCAT(a_users.first_name,' ', SUBSTRING(a_users.middle_name,1,1), '. ', a_users.last_name) AS Name, a_non_billable.trans_date, Sum(a_non_billable.hrs_elapsed) AS SumOfhrs_elapsed, a_non_billable.remarks, a_non_billable_list.non_bill_name, 'xTransDate1' AS StartDate, 'xTransDate2' AS EndDate
FROM (a_non_billable LEFT JOIN a_users ON a_non_billable.user_id = a_users.user_id) LEFT JOIN a_non_billable_list ON a_non_billable.non_bill_type = a_non_billable_list.non_bill_id
WHERE (((a_users.user_id)='xUserID') AND ((a_non_billable.trans_date) BETWEEN 'xTransDate1' AND 'xTransDate2'))
GROUP BY a_users.user_id, a_users.first_name, a_users.middle_name, a_users.last_name, a_non_billable.trans_date, a_non_billable.remarks, a_non_billable_list.non_bill_name
ORDER BY a_non_billable.trans_date DESC;]]></queryString>

