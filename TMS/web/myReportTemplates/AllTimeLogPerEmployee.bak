	<queryString><![CDATA[SELECT a_users.user_id, CONCAT(a_users.first_name, ' ', SUBSTRING(a_users.middle_name, 1, 1), '. ', a_users.last_name) AS Name, a_leaves.leave_name AS ActivityName, a_leave_details.trans_date, a_leave_details.remarks, (a_leave_details.count * 8.0) AS HoursElapsed, 'xTransDate1' AS StartDate, 'xTransDate2' AS EndDate, 'WL' AS ActivityType, a_leaves.leave_id AS myID
FROM a_users RIGHT JOIN (a_leave_details LEFT JOIN a_leaves ON a_leave_details.leave_id = a_leaves.leave_id) ON a_users.user_id = a_leave_details.user_id
WHERE (((a_users.user_id)='2035791') AND ((a_leave_details.trans_date) Between '2000-01-01' And '2010-01-01'))
UNION
SELECT a_users.user_id, CONCAT(a_users.first_name, ' ', SUBSTRING(a_users.middle_name, 1, 1), '. ', a_users.last_name), a_non_billable_list.non_bill_name, a_non_billable.trans_date, a_non_billable.remarks, Sum(a_non_billable.hrs_elapsed) AS SumOfhrs_elapsed, 'xTransDate1' AS Expr1, 'xTransDate2' AS Expr2, 'NB' AS Expr3, a_non_billable_list.non_bill_id
FROM (a_non_billable LEFT JOIN a_users ON a_non_billable.user_id = a_users.user_id) LEFT JOIN a_non_billable_list ON a_non_billable.non_bill_type = a_non_billable_list.non_bill_id
WHERE (((a_users.user_id)='2035791') AND ((a_non_billable.trans_date) Between '2000-01-01' And '2010-01-01'))
GROUP BY a_users.user_id, a_users.first_name, a_users.middle_name, a_users.last_name, a_non_billable_list.non_bill_name, a_non_billable.trans_date, a_non_billable.remarks, 'xTransDate1', 'xTransDate2', 'WL', a_non_billable_list.non_bill_id
UNION
SELECT a_users.user_id, CONCAT(a_users.first_name, ' ', SUBSTRING(a_users.middle_name, 1, 1), '. ', a_users.last_name), CONCAT(a_project.project_name ,' (' , a_def_activity.act_name,')'), a_schedule.trans_date, a_schedule.remarks, a_schedule.hrs_worked, 'xTransDate1' AS Expr1, 'xTransDate2' AS Expr2, 'BI' AS Expr3, CONCAT(a_project.client_id,'-', a_project.project_id, '-',a_def_activity.act_id)
FROM a_def_activity RIGHT JOIN (((a_activity_list LEFT JOIN (a_project RIGHT JOIN a_activity ON a_project.project_id = a_activity.project_id) ON a_activity_list.activity_id = a_activity.activity_id) LEFT JOIN a_users ON a_activity_list.user_id = a_users.user_id) RIGHT JOIN a_schedule ON a_activity_list.activity_list_id = a_schedule.activity_list_id) ON a_def_activity.act_id = a_schedule.activity_type
WHERE (((a_users.user_id)='2035791') AND ((a_schedule.trans_date) Between '2000-01-01' And '2010-01-01'))
GROUP BY a_users.user_id, a_users.first_name, a_users.middle_name, a_users.last_name, a_def_activity.act_name, a_schedule.trans_date, a_schedule.remarks, 'xTransDate1', 'xTransDate2', 'BI', a_project.client_id, a_project.project_id, a_def_activity.act_id
ORDER BY 4, 10]]></queryString>

