	<queryString><![CDATA[SELECT a_users.user_id, CONCAT(a_users.first_name, ' ', SUBSTRING(a_users.middle_name, 1, 1), '. ', a_users.last_name) AS Name, MIN(a_leave_details.trans_date) AS MinDate, MAX(a_leave_details.trans_date) AS MaxDate, a_leaves.leave_name AS myDetails, a_leave_details.remarks, 'xTransDate1' AS StartDate, 'xTransDate2' AS EndDate, SUM(a_leave_details.count) AS NoOfDays, grouping
FROM (a_leave_details LEFT JOIN a_leaves ON a_leave_details.leave_id = a_leaves.leave_id) LEFT JOIN a_users ON a_leave_details.user_id = a_users.user_id
WHERE (((a_leave_details.trans_date) Between '2000-01-01' And '2010-01-01')) AND a_leaves.leave_id LIKE '%'  
GROUP BY a_users.user_id, CONCAT(a_users.first_name, ' ', SUBSTRING(a_users.middle_name, 1, 1), '. ', a_users.last_name), CONCAT(RPAD(a_leaves.leave_name,50,' '), a_leave_details.remarks), StartDate, EndDate, grouping
ORDER BY 2, a_leave_details.trans_date;]]></queryString>

